import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		System.out.println("Enter a number that you wanna calculate its factorial:");
		
		int num = scan.nextInt();
		
		int i = 1;
		int factorial = 1;
	
		while(i<=num) {
			
			factorial *=i;
			
			i++;
		}
		
		System.out.println(num + " factorial equals to " + factorial);
		
	}

}
